+				m.cmd = exec.Command(getFirecrackerBinaryPath(), args...)
+			}, WithLogger(logrus.NewEntry(machineLogger)))
+			require.NoError(t, err)
+
+			err = m.Start(ctx)
+			require.NoError(t, err)
+
+			c.createSnapshot(m, ctx, memPath, snapPath)
+
+			err = m.StopVMM()
+			require.NoError(t, err)
+		})

{
  "compilerOptions": {
    "jsx": "react",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "sourceMap": false,
    "isolatedModules": true,
    "downlevelIteration": true,
    "composite": true,
    "target": "ES2016",
    "baseUrl": ".",
    "lib": ["es2017", "es2019", "dom", "dom.iterable"],
    "rootDirs": [".", "bazel-bin"]
  },
  "exclude": ["bazel-out", "bazel-bin"]
}
